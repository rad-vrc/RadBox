Configs: {
	CMConfigServer: {
		babyChestsCerebralOnly: {
			Tooltip: If true, QoL crafting items cannot be used to craft vanilla or other modded items
			Label: QoL crafting items can only craft Cerebral Mod items
		}

		telekineticFlyingKnife: {
			Tooltip: If true, the Flying Knife will be converted to a telekinetic weapon and gain an extra ability. Requires reload
			Label: Change Flying Knife to telekinetic damage
		}

		knackThrowing: {
			Tooltip: If false, the knack mechanic will only affect this mod's throwing weapons instead of additional throwing weapons from other mods
			Label: Knack affects all mods throwing weapons
		}

		moddedSentries: {
			Tooltip: If true, sentry weapons from other mods will be converted to Defender class and sentry damage. Requires reload
			Label: Change other mod sentries to sentry damage
		}

		vanillaSentries: {
			Tooltip: If true, vanilla sentry related items like weapons, armor, & accessories will be converted to Defender class and sentry damage. Requires reload
			Label: Change vanilla sentries to sentry damage
		}

		RequireLOS: {
			Tooltip: If true, sentry damage sentries (unlike those remaining as summon damage) require line to sight to place, bypassed with a late game accessory
			Label: Sentry placement requires line of sight
		}

		DisplayName: CM Config Server
	}

	CMConfigClient: {
		disableGuideCrafting: {
			Tooltip: If true, removes the recipes for crafting beginner guides, recommended once they're no longer useful
			Label: Disable Beginner Guides recipes
		}

		mouseTimingUI: {
			Tooltip: If true, the Minstrel timing HUD will appear at the mouse instead of the player
			Label: Minstrel Timing HUD above mouse
		}

		cueBuffer: {
			Tooltip: How many frames ahead of the correct timing window the cues will play
			Label: Minstrel Cue Buffer
		}

		disableAudioCue: {
			Tooltip: Stops an audio cue from playing when melodic weapon correct timing starts
			Label: Disable Audio Cue
		}

		disableVisualCue: {
			Tooltip: Stops a visual cue from apparing when melodic weapon correct timing starts
			Label: Disable Visual Cue
		}

		showClassTag: {
			Tooltip: Shows class tags on items directly related to that class
			Label: Show Class Tags
		}

		showUICount: {
			Tooltip: How high your sentry slots has to be before the count will show in the UI. Set to 0 to disable. Can be useful if something increases your sentry slots, but you don't want to see the UI for it
			Label: Sentry Count UI Display
		}

		DisplayName: CM Config Client
	}
}

