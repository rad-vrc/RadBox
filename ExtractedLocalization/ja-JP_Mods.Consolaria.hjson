Configs: {
	ConsolariaConfig: {
		DisplayName: 設定

		Headers: {
			SeasonalEvents: 季節のイベント
			Miscellaneous: その他
			WorldGeneration: ワールド生成
		}

		easterEnabled: {
			Label: システムの日付で季節のコンテンツを制限する（イースター）
			Tooltip:
				'''
				有効にすると、イースター期間中のみ、関連するアイテムや敵が出現します
				4月1日～4月30日
				'''
		}

		thanksgivingEnabled: {
			Label: システム日付による季節コンテンツの制限（感謝祭）
			Tooltip:
				'''
				有効にすると、感謝祭期間中のみ、関連するアイテムや敵が出現します
				11月1日～11月30日
				'''
		}

		smallEventsEnabled: {
			Label: 季節のコンテンツをシステムの日付で制限する（その他のイベント）
			Tooltip: 有効な場合、該当するアイテムや敵は、該当する休日中、またはウィッシュボーン使用時のみ出現します
		}

		vanillaBossMusicEnabled: {
			Label: コンソラリアのボスのためのバニラ・ミュージック
			Tooltip: 有効にすると、カスタムサウンドトラックは、コンソール版で元々使用されていたテーマに置き換えられます
		}

		tizonaZenithIntegrationEnabled: {
			Label: TizonaのZenithへの統合
			Tooltip: 有効にすると、ZenithのクラフトレシピにTizonaが追加されます
		}

		oktoberLocksEnabled: {
			Label: Oktober Locks vanity item
			Tooltip: 有効にすると、オクトーバーフェスト期間中、Clothierでオクトーバーロックを購入できるようになります
		}

		mythicalWyvernKiteVanillaDropruleEnabled: {
			Label: 神話のワイバーンの凧のバニラ・ドロップルール
			Tooltip: 有効化した場合、神話のワイバーンの凧は風の強い日の間、赤い封筒からではなく神話のワイバーンから4%の確率でドロップするようになります
		}

		heartbeatariaIntegrationEnabled: {
			Label: Heartbeataria Modの統合
			Tooltip: 有効にすると、Heartbeataria Mod専用アイテムをRed Envelopeから入手できます
		}

		originalAncientHeroSetRecipeEnabled: {
			Label: オリジナルの古代の勇者セットのクラフトレシピ
			Tooltip: 有効にすると、古代英雄のセットは、煌めきの代わりに織機で絹糸と紫糸を使って作ることができます
		}

		genderRestrictShopEnabled: {
			Label: ジョージのセットとファビュラスのセットをプレイヤーの性別で制限します
			Tooltip:
				'''
				有効にすると、ブラッドムーンの間、プレイヤーの性別が特定の場合のみ、関連するセットがクロティエのショップに出現します
				男性 - ジョージのセット
				女性 - ファビュラスセット
				'''
		}

		pyramidEnabled: {
			Label: ピラミッドの確定生成
			Tooltip: 有効にすると、ワールドに必ず1つ以上のピラミッドが生成されます
		}

		pyramidMessageLoot: {
			Label: 特別なピラミッド戦利品
			Tooltip: 有効にすると、2番目のピラミッドの宝箱に特別なメッセージが表示されます
		}
	}
}

